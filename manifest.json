{
  "manifest_version": 3,
  "name": "Hello World Extension",
  "version": "1.0",
  "description": "一个简单的Hello World示例",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
